CREATE OR REPLACE SUMMARY VIEW s_order_count (
        customer_code:text (sourcetypeid = '-9', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '20'),
        product_id:int (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '10'),
        main_country:text (sourcetypeid = '-9', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '100'),
        cnt_per_cust_per_product:long (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '19'),
        cnt_item_per_cust_per_product:long (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '19')
    )
    FOLDER = '/7 - summaries'
    ADD SEARCHMETHOD s_order_count(
        I18N us_pst
        CONSTRAINTS (
             ADD customer_code (any) OPT ANY
             ADD product_id (any) OPT ANY
             ADD main_country (any) OPT ANY
             ADD cnt_per_cust_per_product (any) OPT ANY
             ADD cnt_item_per_cust_per_product (any) OPT ANY
        )
        OUTPUTLIST (cnt_item_per_cust_per_product, cnt_per_cust_per_product, customer_code, main_country, product_id
        )
        WRAPPER (jdbc s_order_count)
    )
AS SELECT customer_code AS customer_code, product_id AS product_id, main_country AS main_country, count(DISTINCT order_id) AS cnt_per_cust_per_product, count(id) AS cnt_item_per_cust_per_product FROM iv_orders_customer_detail GROUP BY customer_code, product_id, main_country;

CHOWN admin VIEW s_order_count LASTMODIFICATIONUSER admin;

