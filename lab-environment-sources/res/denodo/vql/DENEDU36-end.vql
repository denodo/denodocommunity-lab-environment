CREATE OR REPLACE RESOURCE_MANAGER RULE "non admin rule" DESCRIPTION = 'This rule is meant for non administrator users' CONDITION admin_user = false RESOURCE_MANAGER PLAN "stop queries that return more than 100 rows";

