CREATE OR REPLACE RESOURCE_MANAGER PLAN "stop all queries" CONDITION true ACTION 'STOP_QUERY_ALWAYS';

CREATE OR REPLACE RESOURCE_MANAGER PLAN "stop queries that return more than 100 rows" DESCRIPTION = 'This plan allows users to return only the first 100 rows of a query and stops user queries if the load of the server is very high' CONDITION true ACTION 'STOP_QUERY_WHEN_MAX_NUMBER_OF_RETURNED_ROWS_REACHED' PARAMETERS ('MaxReturnedRows' = 100) CONDITION server_cpu >= 0.750000 SERVER_CPU_WINDOW_SIZE 35 ACTION 'STOP_QUERY_ALWAYS';

