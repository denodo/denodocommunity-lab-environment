        department_id:int (sourcetypeid = '5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '5')
    )
    FOLDER = '/7 - summaries'
    ADD SEARCHMETHOD sum2_bv_hr_employees_bv_hr_departments(
        I18N us_pst
        CONSTRAINTS (
             ADD department_name (any) OPT ANY
             ADD location_id (any) OPT ANY
             ADD employee_id (any) OPT ANY
             ADD first_name (any) OPT ANY
             ADD last_name (any) OPT ANY
             ADD email (any) OPT ANY
             ADD phone_number (any) OPT ANY
             ADD hire_date (any) OPT ANY
             ADD job_id (any) OPT ANY
             ADD salary (any) OPT ANY
             ADD commission_pct (any) OPT ANY
             ADD manager_id (any) OPT ANY
             ADD department_id (any) OPT ANY
        )
        OUTPUTLIST (commission_pct, department_id, department_name, email, employee_id, first_name, hire_date, job_id, last_name, location_id, manager_id, phone_number, salary
        )
        WRAPPER (jdbc sum2_bv_hr_employees_bv_hr_departments)
    )
AS SELECT bv_hr_departments.department_name AS department_name, bv_hr_departments.location_id AS location_id, bv_hr_employees.employee_id AS employee_id, bv_hr_employees.first_name AS first_name, bv_hr_employees.last_name AS last_name, bv_hr_employees.email AS email, bv_hr_employees.phone_number AS phone_number, bv_hr_employees.hire_date AS hire_date, bv_hr_employees.job_id AS job_id, bv_hr_employees.salary AS salary, bv_hr_employees.commission_pct AS commission_pct, bv_hr_employees.manager_id AS manager_id, bv_hr_employees.department_id AS department_id FROM bv_hr_employees AS bv_hr_employees INNER JOIN bv_hr_departments AS bv_hr_departments ON (bv_hr_employees.department_id = bv_hr_departments.department_id AND bv_hr_employees.employee_id = bv_hr_departments.manager_id)  WHERE (bv_hr_employees.employee_id > 10 AND bv_hr_employees.salary < 15000);

